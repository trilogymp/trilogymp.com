{
  "name": "@trilogymp/website",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/trilogymp/trilogymp.com",
  "bugs": "https://github.com/trilogymp/trilogymp.com/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/trilogymp/trilogymp.com.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "next build",
    "clean": "del-cli ./{dist,out} ./src/cfworker/worker",
    "cleaninstall": "npm run clean && del-cli \"{node_modules,.eslintcache,.husky,.next}\" && npm install",
    "dev": "next dev",
    "lint:prettierbase": "prettier \"./**/*.js\"",
    "lint:prettier:fix": "npm run lint:prettierbase -- --write",
    "lint:prettier:check": "npm run lint:prettierbase -- --debug-check",
    "lint:eslint": "eslint ./ --cache",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:stylelint": "stylelint \"./**/*.{s,}css\"",
    "lint:stylelint:fix": "npm run lint:stylelint -- --fix",
    "lint": "npm-run-all lint:prettier:check lint:eslint lint:stylelint",
    "lintfix": "npm-run-all lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "postinstall": "npm-run-all --parallel postinstall:husky postinstall:playwright",
    "postinstall:husky": "husky install",
    "postinstall:playwright": "playwright install",
    "prepare": "husky install",
    "start": "next start",
    "test": "npm-run-all lint test:playwright",
    "test:playwright": "playwright test src/tests",
    "export": "next build && next export",
    "workers:publish": "npm-run-all --parallel workers:publish:dev workers:publish:prod",
    "workers:publish:dev": "wrangler publish",
    "workers:publish:prod": "wrangler publish --env production",
    "wrangler": "wrangler",
    "storybook": "npm-run-all export storybook:start",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "npm-run-all build build-storybook"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "^12.0.1",
    "next-seo": "^4.28.1",
    "postcss": "^8.3.11",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-rtlcss": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "alpha",
    "react-dom": "alpha",
    "react-i18next": "^11.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@cloudflare/wrangler": "^1.19.4",
    "@playwright/test": "^1.16.2",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.4.0-beta.21",
    "@storybook/addon-essentials": "^6.4.0-beta.21",
    "@storybook/addon-links": "^6.4.0-beta.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.0-beta.21",
    "@storybook/manager-webpack5": "^6.4.0-beta.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.0-beta.21",
    "@washingtonpost/storybook-addon-web-vitals": "^0.1.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.0",
    "del": "^6.0.0",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "storybook-addon-designs": "^6.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-recommended-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "webpack": "^5.60.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
